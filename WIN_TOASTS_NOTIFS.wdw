        code : |1+
         
         // Local variables
         nVerticalAlignment		is int
         nHorizontalAlignment	is int
         oNotif					is Notification
         
         <compile if TypeConfiguration=iOS>
         	
         	// Position of the toast
         	switch MySelf
         		// 
         		case 4
         			nVerticalAlignment		= vaTop
         			nHorizontalAlignment	= haCenter
         		// 
         		case 5
         			nVerticalAlignment		= vaMiddle
         			nHorizontalAlignment	= haCenter			
         		// 
         		case 6
         			nVerticalAlignment		= vaBottom
         			nHorizontalAlignment	= haCenter
         	end
         	
         <else>
         	
         	
         	// Position of the toast
         	switch MySelf
         		case 1
         			nVerticalAlignment		= vaTop
         			nHorizontalAlignment	= haLeft
         		case 2
         			nVerticalAlignment		= vaMiddle
         			nHorizontalAlignment	= haLeft
         		case 3
         			nVerticalAlignment		= vaBottom
         			nHorizontalAlignment	= haLeft
         		case 4
         			nVerticalAlignment		= vaTop
         			nHorizontalAlignment	= haCenter
         		case 5
         			nVerticalAlignment		= vaMiddle
         			nHorizontalAlignment	= haCenter
         		case 6
         			nVerticalAlignment		= vaBottom
         			nHorizontalAlignment	= haCenter
         		case 7
         			nVerticalAlignment		= vaTop
         			nHorizontalAlignment	= haRight
         		case 8
         			nVerticalAlignment		= vaMiddle
         			nHorizontalAlignment	= haRight
         		case 9
         			nVerticalAlignment		= vaBottom
         			nHorizontalAlignment	= haRight
         		other case
         			nVerticalAlignment		= vaBottom
         			nHorizontalAlignment	= haCenter
         	end
         	
         <end>
         
         // Display the toast
         ToastDisplay("This is a toast" + CR + "multiline !", toastShort, nVerticalAlignment, nHorizontalAlignment)
         
         // Add a notification
         oNotif.Title	= "New WINDEV Mobile notification"
         oNotif.Message	= "Creating a notification with WINDEV Mobile is child's play."
         
         <compile if TypeConfiguration = Android>
         	
         	oNotif.DropDownText			= "New WINDEV Mobile notification"
         	oNotif.DisplayLED			= True
         	oNotif.Deletable			= True
         	oNotif.Vibration			= True
         	oNotif.ActivateApplication	= True
         	
         <end>
         
         oNotif.Sound		= notifDefaultSound
         oNotif.ActionClick	= ClickOnNotification
         NotifAdd(oNotif)
