     procedure_id : 1295168227333274347
     type_code : 14
     code : |1-
      // Summary: Returns the path of last photo that was taken
      // Syntax:
      //[ <Result> = ] LastPhoto ()
      //
      // Parameters:
      //	None
      
      // Return value:
      // 	string: Path of last photo that was taken
      procedure LastPhoto() 
      
      sImage		is string
      sListFile	is string
      
      sListFile = fListFile(gsDirectory + [ "/" ] +  "*.jpg", frNotRecursive)
      
      // For each file found
      for each string AFile of sListFile SEPAREE by CR
      	// If the image is more recent, keep it
      	if sImage = "" _or_ AFile > sImage then
      		sImage = AFile
      	end
      end
      
      return sImage
     type : 458752
