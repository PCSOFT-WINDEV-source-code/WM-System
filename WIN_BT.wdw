      // Summary: Refreshes the window
      //
      procedure Refresh() 
      
      if BTStatus() = btDisabled then
      	BTN_Activate = "Enable the Bluetooth"
      else
      	BTN_Activate = "Disable the Bluetooth"
      end
      
      RefreshVisibilityCBox()
      
      DisplayDevices()
     type : 458752
   -
     name : RefreshVisibilityCBox
     procedure_id : 1295168205856073384
     type_code : 14
     code : |1-
      // Summary: Refreshes the check box for visibility
      // Syntax:
      //RefreshVisibilityCBox ()
      //
      // Parameters:
      //	None
      // Example:
      // Indicate an example.
      //
      // Return value:
      procedure RefreshVisibilityCBox() 
      
      if BTStatus() = btDisabled then
      	CBOX_Visibility.State	= Grayed
      	CBOX_Visibility			= False
      else
      	CBOX_Visibility.State	= Active
      	CBOX_Visibility			= BTIsVisible()
      end
     type : 458752
   -
     name : DisplayDevices
     internal_properties : CAAAAAgAAACm6U4eBqjNikU6aUWNqzUjTVEStTI1CPAi8GoQqgaGxkskTlkbh0EeeyuBQ53o/PIcKktXMP/oLzB7TgrOTyN1cgJs4oqPRKcPn7WwovruemlDHp6nG0F5UYuf4Iv5Mzbp2oE6ANqkti4a1Yk5Lh5P30y82NO4MBZh008B9suXNbC2
     procedure_id : 1295168205856138920
     type_code : 14
     code : |1-
      // Summary: Displays the available devices
      
      procedure DisplayDevices() 
      sTmpString		is string
      sListDevices	is string
      
      if BTStatus() = btEnabled then
      	
      	// Returns the list of devices
      	sListDevices = BTListDevice()
      	
      	// Clears the looper where the devices are listed
      	LooperDeleteAll(LOOP_Devices)
      	// Fills the looper
      	if sListDevices <> "" then
      		for each string sTmpString of sListDevices SEPAREE by CR
      			// Retrieve the values
      			sIdentifier	is string	= ExtractString(sTmpString,1, TAB)
      			sMACAddress	is string	= ExtractString(sTmpString,2, TAB)
      			sName		is string	= ExtractString(sTmpString,3, TAB)
      			if sName <> "" then
      				LooperAdd(LOOP_Devices,sTmpString)
      			end
      		end
      	else
      		Info(<§$0000§>) 
      	end
      end
     type : 458752
