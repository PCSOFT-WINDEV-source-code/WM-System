        code : |1-
         
         // Display the power
         nPower is int = ATT_Power
         if nPower < -90 then
         	ATT_ImagePower = IMG_P1
         else
         	if nPower < -75 then
         		ATT_ImagePower = IMG_P2
         	else
         		if nPower < -60 then
         			ATT_ImagePower = IMG_P3
         		else
         			ATT_ImagePower = IMG_P4
         		end
         	end
         end
         
         
         // Display an indicator of active connection
         if ATT_Connected = "Connected" then
         	ATT_ImageConnected = IMG_On
         else
         	ATT_ImageConnected = IMG_Off
         end
