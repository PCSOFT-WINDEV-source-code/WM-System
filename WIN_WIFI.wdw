      -
        code : |1+
         
         sIdentifier	is string
         sPassword	is string
         bRes		is boolean
         
         sIdentifier = ATT_Identifier
         
         if ATT_Connected <> "Connected" then
         	//---------------------------------------------------------CONNECTION------------------------------------------------------
         	
         	switch Dialog(<§?1CA3307900B5898E§> , LOOP_AvailWifiNetworks[LOOP_AvailWifiNetworks].ATT_SSID)
         		// Connect
         		case 1
         			if sIdentifier <> "" then
         				// The network is part of the networks that are already configured
         				bRes = WiFiConnect(sIdentifier) 
         			else
         				// The network is not part of the networks that are already configured, it must be added
         				sSecurity is string
         				
         				// Retrieve the security
         				if ATT_Security = <§$0002§> then
         					sSecurity = wifiSecurityNone
         				else
         					sSecurity = ATT_Security
         				end
         				
         				if sSecurity <> wifiSecurityNone then
         					// The network is secured
         					switch Input(<§$0003§>,sPassword)
         						// OK
         						case 1
         							// Add the network into the phone
         							// In this example, the network addition is not permanent therefore the "Permanent" parameter is set to False
         							sIdentifier = WiFiAddNetwork(ATT_SSID, ATT_BSSID, sSecurity, sPassword, False)
         						// Cancel
         						case 2
         					end
         				else
         					// Add the network into the phone
         					// In this example, the network addition is not permanent therefore the "Permanent" parameter is set to False
         					sIdentifier = WiFiAddNetwork(ATT_SSID, ATT_BSSID, sSecurity, "", False)
         				end
         				
         				
         				// If the network was successfully added, connect
         				if sIdentifier <> ""  then
         					bRes = WiFiConnect(sIdentifier)
         				end
         			end
         			
         			if bRes then
         				Info(<§$0004§>)
         			else
         				Info(<§$0005§>)
         			end
         	end
         	
         else
         	
         	//---------------------------------------------------------DISCONNECTION------------------------------------------------------
         	//1: Disconnect
         	//2: Don't disconnect
         	switch Dialog(<§?1CA34E7F01D0D205§>)
         		// Disconnect
         		case 1
         			WiFiDeleteNetwork(sIdentifier)
         			
         			Refresh()
         	end
         	
         end
         
         
         // Refresh
         Refresh()
         
         
         
         
