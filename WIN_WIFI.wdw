      // Summary: Refreshes the window
      
      procedure Refresh() 
      
      if WiFiStatus() = wifiDisabled then
      	BTN_Activate		= "Enable the Wifi"
      	OPT_Option.State	= Grayed
      else
      	BTN_Activate		= "Disable the Wifi"
      	OPT_Option.State	= Active
      	
      	// Retrieves the identifier of the current connection
      	gsCurrentID			= WiFiConnectionInfo(wifiIdNetwork)
      end
      
      DisplayWifiNetworks()
      
