   -
     name : DisplayWifiNetworks
     internal_properties : CAAAAAgAAAC7kmPFAacsd9sTHIGKggrnsIMRzgve03CrAbthxwclYnJ3jWKtJGukYqpeq7EEIaTrv+ln/GV6jyA5/eZv95eE7cG1vawbtdtxapb7dXN6EYtIeZ8Rq3wV8YYQq3iYt7XTbRTLsu5DkrtuSVLvF1QeNVxQ6GKEIHy5clZvxP00Nw==
     procedure_id : 1295168223037978496
     type_code : 14
     code : |1-
      // Summary: Displays the available WIFI networks in the looper
      // Syntax:
      //DisplayWifiNetworks ()
      //
      //
      procedure DisplayWifiNetworks() 
      
      // Delete the former networks
      LooperDeleteAll(LOOP_AvailWifiNetworks)
      
      if WiFiStatus() = wifiEnabled then
      	// Detect the available WIFI networks
      	sList is string = WiFiDetectAccessPoint()
      	for each string sAccessPoint of sList SEPAREE by CR
      		// Retrieve the values
      		sIdentifier	is string	= ExtractString(sAccessPoint,1, TAB)
      		sSSID		is string	= ExtractString(sAccessPoint,2, TAB)
      		sBSSID		is string	= ExtractString(sAccessPoint,3, TAB)
      		sSecurity	is string	= ExtractString(sAccessPoint,4, TAB)
      		sPower		is string	= ExtractString(sAccessPoint,5, TAB)
      		sConnected	is string	= ""
      		
      		if sSecurity = wifiSecurityNone then
      			sSecurity = <§$0000§>
      		end
      		
      		// Are we connected to this network
      		if gsCurrentID = sIdentifier and gsCurrentID <> "" then
      			sConnected = <§$0001§>
      		end
      		
      		// Add the network into the looper
      		LooperAdd(LOOP_AvailWifiNetworks,sSSID + TAB + sSecurity + TAB + sPower + TAB + sIdentifier + TAB + sBSSID + TAB + sConnected)
      	end
      	LOOP_AvailWifiNetworks.Visible = True
      else
      	LOOP_AvailWifiNetworks.Visible = False
      end
