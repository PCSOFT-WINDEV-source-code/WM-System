   -
     name : DetectAcceleration
     procedure_id : 1295168171493050450
     type_code : 14
     code : |1+
      // Summary: Detects the acceleration
      // Syntax:
      //DetectAcceleration (<rVertical> is real, <rLongitudinal> is real, <rLateral> is real)
      //
      // Parameters:
      //	rVertical (real): corresponds to the  measurement of the device acceleration on the vertical axis (Z) in m/s²
      //	rLongitudinal (real): corresponds to the measurement of the device acceleration on the longitudinal axis (Y) in m/s²
      //	rLateral (real): corresponds to the measurement of the device acceleration on the horizontal axis (X) in m/s²
      // Example:
      // Indicate an example.
      //
      // Return value:
      procedure DetectAcceleration(rVertical is real, rLongitudinal is real, rLateral is real) 
      
      grAccLateral		= rLateral
      grAccLongitudinal	= rLongitudinal 
      grAccVertical		= rVertical
      
     type : 458752
   -
     name : InitializeTheGame
     procedure_id : 1295168171493115986
     type_code : 14
     code : |1+
      // Summary: Initializes the game
      // Syntax:
      //InitializeTheGame ()
      //
      //
      procedure InitializeTheGame() 
      
      gnMaxLateralSpeed	= 0
      PROGBAR_Power		= 0
      
      grAccVertical		= 0
      grAccLongitudinal	= 0
      grAccLateral		= 0
      
     type : 458752
   -
     name : ProcessLateralSpeed
     procedure_id : 1295168171493181522
     type_code : 14
     code : |1-
      // Summary: Displaying the game
      // Syntax:
      //ProcessLateralSpeed (<nMaxLateralSpeed> is int)
      //
      procedure ProcessLateralSpeed(nMaxLateralSpeed is int) 
      
      // If the lateral speed is greater than the maximum one, change the maximum speed
      if nMaxLateralSpeed > gnMaxLateralSpeed and nMaxLateralSpeed > 1 then
      	PROGBAR_Power		= nMaxLateralSpeed * 5
      	gnMaxLateralSpeed	= nMaxLateralSpeed
      end
      
      // Display the note
      STC_Note = PROGBAR_Power + "/100"
      
      // Change the comment
      if PROGBAR_Power >= 70 then
      	STC_Comment = <§$0000§>
      else
      	if PROGBAR_Power >= 50 then
      		STC_Comment = <§$0001§>
      	else
      		if PROGBAR_Power >= 30 then
      			STC_Comment = <§$0002§>
      		else
      			STC_Comment = <§$0003§>
      		end
      	end
      end
     type : 458752
   -
     name : UIUpdate
     internal_properties : CAAAAAgAAAAAU6ulE8z2T7NxaKH1bduq4k3yAC7oLCQ7OClVongR2b4wI0npbgfPVKLmXo69L0Jt2vs0c2zzhK9GEn2ZWFLFVHq6BnYDtxhkl0mH+PFVBxQhgisBDlobpXhKoaczIlSPTBzB85eCgknjzcN5cmQoXHToMjyw/XmzmzTB91jNnqcJAQ==
     procedure_id : 1295168171493247058
     type_code : 14
     code : |1-
      // Summary: Update interface periodically
      // Syntax:
      //UIUpdate ()
      //
      // Parameters:
      //	None
      // Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be repeated in loop with 20 hundredths of a second between each call
      // Each following call runs the procedure once, without timer
      // 
      
      procedure UIUpdate()
      
      // Display the speeds
      EDT_Vertical		= NumToString(grAccVertical,".2f")
      EDT_Longitudinal	= NumToString(grAccLongitudinal,".2f")
      EDT_Lateral			= NumToString(grAccLateral,".2f")
      
      // Process the lateral speed
      ProcessLateralSpeed(Abs(IntegerPart(grAccLateral)))
     type : 458752
